{
  "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc","closure"]
  },
  "source": {
      "include": ["./src"]
  },
  "plugins": [
      "plugins/markdown",
      "better-docs/component",
      "node_modules/@ckeditor/jsdoc-plugins/lib/export-fixer/export-fixer"
  ],
  "opts": {
      "encoding": "utf8",
      "destination": "docs/",
      "recurse": true,
      "verbose": true,
      "template": "./node_modules/better-docs"
  },
  "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
  }
}
